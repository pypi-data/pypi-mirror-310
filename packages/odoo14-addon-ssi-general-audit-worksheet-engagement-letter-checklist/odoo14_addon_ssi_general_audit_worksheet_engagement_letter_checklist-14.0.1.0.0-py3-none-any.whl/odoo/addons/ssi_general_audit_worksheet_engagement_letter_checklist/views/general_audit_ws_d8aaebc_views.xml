<odoo>

    <record id="general_audit_ws_d8aaebc_view_search" model="ir.ui.view">
        <field name="name">general_audit_ws_d8aaebc form</field>
        <field name="model">general_audit_ws_d8aaebc</field>
        <field
            name="inherit_id"
            ref="ssi_general_audit.general_audit_worksheet_mixin_view_search"
        />
        <field name="mode">primary</field>
        <field name="arch" type="xml">
            <data />
        </field>
    </record>

    <record id="general_audit_ws_d8aaebc_view_tree" model="ir.ui.view">
        <field name="name">general_audit_ws_d8aaebc tree</field>
        <field name="model">general_audit_ws_d8aaebc</field>
        <field name="mode">primary</field>
        <field
            name="inherit_id"
            ref="ssi_general_audit.general_audit_worksheet_mixin_view_tree"
        />
        <field name="arch" type="xml">
            <data>
            </data>
        </field>
    </record>

    <record id="general_audit_ws_d8aaebc_view_form" model="ir.ui.view">
        <field name="name">general_audit_ws_d8aaebc form</field>
        <field name="model">general_audit_ws_d8aaebc</field>
        <field name="mode">primary</field>
        <field
            name="inherit_id"
            ref="ssi_general_audit.general_audit_worksheet_mixin_view_form"
        />
        <field name="arch" type="xml">
            <data>
                <xpath expr="//page[1]" position="before">
                    <page name="checklist" string="Checklist">
                        <field name="checklist_ids">
                            <tree editable="top">
                                <field name="sequence" widget="handle" />
                                <field name="checklist_id" />
                                <field name="checklist_ok" widget="boolean_toggle" />
                            </tree>
                        </field>
                    </page>
                </xpath>
            </data>
        </field>
    </record>

    <record id="general_audit_ws_d8aaebc_action" model="ir.actions.act_window">
        <field name="name">Engagement Letter Checklist</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">general_audit_ws_d8aaebc</field>
        <field name="view_mode">tree,form</field>
    </record>

    <menuitem
        id="general_audit_ws_d8aaebc_menu"
        name="Engagement Letter Checklist"
        parent="ssi_general_audit.menu_general_audit_preliminary_engagement"
        action="general_audit_ws_d8aaebc_action"
        sequence="1503"
        groups="general_audit_ws_d8aaebc_viewer_group"
    />


</odoo>
