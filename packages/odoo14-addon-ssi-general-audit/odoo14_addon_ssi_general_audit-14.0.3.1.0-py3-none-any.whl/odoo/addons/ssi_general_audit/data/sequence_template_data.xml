<odoo>

<!-- General Audit -->
<data noupdate="1">
<record id="sequence_template_general_audit" model="sequence.template">
    <field name="name">Standard</field>
    <field name="model_id" ref="ssi_general_audit.model_general_audit" />
    <field name="sequence" eval="100" />
    <field name="initial_string">/</field>
    <field
                name="sequence_field_id"
                search="[('model_id.model','=','general_audit'),('name','=','name')]"
            />
    <field
                name="date_field_id"
                search="[('model_id.model','=','general_audit'),('name','=','date_start')]"
            />
    <field name="computation_method">use_python</field>
    <field name="python_code">result=True</field>
    <field name="sequence_id" ref="sequence_general_audit" />
    <field name="sequence_selection_method">use_sequence</field>
    <field name="add_custom_prefix" eval="0" />
    <field name="add_custom_suffix" eval="0" />
</record>
</data>

