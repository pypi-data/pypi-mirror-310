    "import inspect\n",
    "\n",
    "from ayy import tools\n",
    "\n",
    "\n",
    "def get_functions_from_module(module):\n",
    "    return [\n",
    "        x for x in inspect.getmembers(module, lambda x: inspect.isfunction(x) and x.__module__ == module.__name__)\n",
    "    ]\n",
    "\n",
    "\n",
    "get_functions_from_module(tools)\n"
