class Pagination(FormComponent):
    """
    Creates a pagination component for navigating through pages of data.
    """

    EVENTS = [
        Events.change,
    ]

    def __init__(
        self,
        value: str | Callable | None = None,
        *,
        label: str | None = None,
        every: float | None = None,
        show_label: bool | None = None,
        scale: int | None = None,
        min_width: int = 160,
        interactive: bool | None = None,
        visible: bool = True,
        elem_id: str | None = None,
        elem_classes: list[str] | str | None = None,
        render: bool = True,
        key: int | str | None = None,
        total: int = 0,  # 总数据条数
        page: int = 1,  # 当前页码
        page_size: int = 10,  # 每页条数
        change: Callable[[int, int], None] = lambda page, size: None,
    ):
        self.total = total
        self.page = page
        self.page_size = page_size
        self.change = change  # Set change to user-provided function or default to no-op
        super().__init__(
            label=label,
            every=every,
            show_label=show_label,
            scale=scale,
            min_width=min_width,
            interactive=interactive,
            visible=visible,
            elem_id=elem_id,
            elem_classes=elem_classes,
            value=value,
            render=render,
            key=key,
        )

    def pagination_range(self, current_page: int, total_page: int) -> list[int | str]:
        range_ = []
        delta = 2
        left = max(2, current_page - delta)
        right = min(total_page - 1, current_page + delta)

        if left > 2:
            range_.extend([1, "..."])
        else:
            range_.append(1)

        range_.extend(range(left, right + 1))

        if right < total_page - 1:
            range_.extend(["...", total_page])
        elif right == total_page - 1:
            range_.append(total_page)

        return range_

    def update_page(self, new_page: int):
        print('update_page:', new_page)  # 打印payload
        if 1 <= new_page <= self.max_page():
            self.page = new_page
            if self.change:
                self.change(self.page, self.page_size)

    def update_page_size(self, new_size: int):
        print('update_page_size:', new_size)  # 打印payload
        if new_size > 0:
            self.page_size = new_size
            self.page = 1  # Reset to first page
            if self.change:
                self.change(self.page, self.page_size)

    def max_page(self) -> int:
        return (self.total + self.page_size - 1) // self.page_size  # Ceiling division

    def preprocess(self, payload: dict[str, int]) -> None:
        print('preprocess:', payload)  # 打印payload
        """
        Parameters:
            payload: Triggers page or page_size update.
        """
        if payload.get("page") is not None:
            self.update_page(payload["page"])
        if payload.get("page_size") is not None:
            self.update_page_size(payload["page_size"])

    def postprocess(self, value: str | None) -> str | None:
        print('postprocess:', value)  # 打印value
        """
        Parameters:
            value: Expects a {str} returned from function and sets textarea value to it.
        Returns:
            The value to display in the textarea.
        """
        return None if value is None else str(value)

    def api_info(self) -> dict[str, Any]:
        return {"type": "string"}

    def example_payload(self) -> Any:
        return {"page": 1, "page_size": 10, "total": 100}

    def example_value(self) -> Any:
        return {"page": 1, "page_size": 10, "total": 100}
