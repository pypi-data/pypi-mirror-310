        self.rtl = rtl
        self.fileName = fileName
        self.fileContent = fileContent
        self.width = width
        self.height = height
        self.appAccessKey = appAccessKey
        self.clientName = clientName
        super().__init__(
            label=label,
            every=every,
            show_label=show_label,
            scale=scale,
            min_width=min_width,
            interactive=interactive,
            visible=visible,
            elem_id=elem_id,
